<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="boardBizImpl" class="com.midas.websolution.board.biz.BoardBizImpl">
		<property name="boardDao" ref="boardDaoImpl"></property>
	</bean>
	
	<bean id="boardServiceImpl"
		class="com.midas.websolution.board.service.BoardServiceImpl">
	<property name="boardBiz" ref="boardBizImpl"></property>
	</bean>

	<bean id="junsuServiceImpl"
		class="com.midas.websolution.board.service.JunsuServiceImpl">
	</bean>
	
	
	<bean id="boardDaoImpl" class="com.midas.websolution.board.dao.BoardDaoImpl">
		<property name="sqlSessionTemplate" ref="sqlSessionTemplate"></property>
	</bean>
	
	<bean id="elonServiceImpl" class="com.midas.websolution.board.service.ElonServiceImpl">
		<property name="elonDao" ref="elonDaoImpl"></property>
	</bean>
	
	<bean id="elonDaoImpl" class="com.midas.websolution.board.dao.ElonDaoImpl">
		<property name="sqlSessionTemplate" ref="sqlSessionTemplate"></property>
	</bean>

</beans>
